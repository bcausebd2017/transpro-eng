body{
	font-size: 12px;
	color: #333;
	line-height: 18px;
}

.p5 {
	padding-left: 5px;
	padding-right: 5px;
}
.pt25 {
	padding-top: 25px;
}
.pt15 {
	padding-top: 15px;
}
.site-title {
    background: #45b3dd none repeat scroll 0 0;
    display: block;
    overflow: hidden;
}
.site-title i.fa {
    background: #3d85b4 none repeat scroll 0 0;
    color: #fff;
    display: inline-block;
    float: left;
    font-size: 18px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    width: 36px;
}
.site-title h3 {
    color: #fff;
    font-size: 15px;
    font-weight: bold;
    line-height: 36px;
    margin: 0;
    padding-left: 50px;
}








/* header-top-area */
.header-top-area {
    padding: 5px 0;
}
.top-left p {
    font-size: 12px;
    margin: 0;
}
.top-right ul {
    display: inline-block;
    list-style: outside none none;
    margin: 0 20px 0 0;
    padding: 0;
}
.top-right ul li {
    border-right: 1px solid #333;
    display: inline-block;
    line-height: 12px;
    padding-left: 10px;
    padding-right: 10px;
}
.top-right ul li:last-child {
    border-right: none;
}
.top-right ul li a {
    color: #333;
    text-decoration: none;
}
.top-right ul li a:hover {
    color: #0078d7;
}
.top-right a.top-btn {
    background: #00aeef none repeat scroll 0 0;
    color: #fff;
    display: inline-block;
    padding: 5px 10px;
    text-decoration: none;
}
.top-right a.top-btn:hover {
    background: #0078d7 none repeat scroll 0 0;
}





/* tab menu */
.tab-menu-area {
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#01b9fe+0,0075bf+100 */
	background: #01b9fe; /* Old browsers */
	background: -moz-linear-gradient(top, #01b9fe 0%, #0075bf 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #01b9fe 0%,#0075bf 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #01b9fe 0%,#0075bf 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#01b9fe', endColorstr='#0075bf',GradientType=0 ); /* IE6-9 */
	border-bottom: 1px solid #004982;	
}
.tab-menu ul {
    list-style: outside none none;
    margin: 0;
    padding: 0;
}
.tab-menu ul li {
	border-left: 1px solid #2ec5fe;
	border-right: 1px solid #007ead;
	display: inline-block;
	float: left;
}
.tab-menu ul li a {
	color: #fff;
	display: block;
	font-weight: bold;
	padding: 15px 29px;
	text-decoration: none;
}
.tab-menu ul li a:hover {
	background: #0075bf; /* Old browsers */
	background: -moz-linear-gradient(top, #0075bf 0%, #01b9fe 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #0075bf 0%,#01b9fe 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #0075bf 0%,#01b9fe 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0075bf', endColorstr='#01b9fe',GradientType=0 ); /* IE6-9 */
}


.nav-tabs > li > a
{
    padding:5px 5px;
    font-size:12px;
}
/* main-menu-area */
.main-menu-area {
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#01b9fe+0,0075bf+100 */
	background: #45b3dd; /* Old browsers */
	background: -moz-linear-gradient(top, #45b3dd 0%, #0075bf 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #45b3dd 0%,#0075bf 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #45b3dd 0%,#0075bf 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#01b9fe', endColorstr='#0075bf',GradientType=0 ); /* IE6-9 */
	border-bottom: 1px solid #004982;
}


.main-menu {
    padding-left: 4%;
}
.main-menu ul {
    list-style: outside none none;
    margin: 0;
    padding: 0;
}
.main-menu ul li {
	border-left: 1px solid #2ec5fe;
	border-right: 1px solid #007ead;
	display: inline-block;
	float: left;
}
.main-menu ul li a {
	display: block;
	padding: 15px 12px;
	text-decoration: none;
}
.main-menu ul li a:hover {
	background: #0075bf; /* Old browsers */
	background: -moz-linear-gradient(top, #0075bf 0%, #01b9fe 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #0075bf 0%,#01b9fe 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #0075bf 0%,#01b9fe 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0075bf', endColorstr='#01b9fe',GradientType=0 ); /* IE6-9 */
}

.menu-item {
    padding-left: 36px;
    position: relative;
    text-align: center;
}
.menu-item i.fa {
	border-right: 1px dashed #B1B0B0;
	color: #fff;
	font-size: 16px;
	left: 0;
	padding-right: 8px;
	position: absolute;
	text-shadow: 2px 2px 4px #333;
	top: 0;
}
.menu-item p {
    color: #fff;
    font-size: 12px;
    font-weight: bold;
    margin: 0;
    text-transform: uppercase;
}
.menu-item span {
    color: #000;
    font-size: 10px;
    text-transform: uppercase;
}



.mobile-menu-area {
    display: none;
}

.tab-menu-area {
    display: none;
}







.slider-area {
	background: #91e2ff; /* Old browsers */
	background: -moz-linear-gradient(top, #91e2ff 0%, #ffffff 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #91e2ff 0%,#ffffff 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #91e2ff 0%,#ffffff 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#91e2ff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}

.single-slider {
    margin-top: 5px;
}
.single-slider img {
    width: 100%;
}






.total-content {
    margin-top: 15px;
}

/* pricing-table-area */
.pricing-table-area {
    margin-bottom: 35px;
}
.pricing-table-area p {
    background: #f6f6f6 none repeat scroll 0 0;
    border: 1px solid #ddd;
    font-size: 13px;
    line-height: 18px;
    margin-bottom: 15px;
    padding: 15px;
}
.single-pricing-box {
    position: relative;
    text-align: center;
}
.pricing-photo img {
    width: 100%;
}
.pricing-btn {
    bottom: -10px;
    position: absolute;
    width: 100%;
}
.pricing-btn a {
    background: #aaa none repeat scroll 0 0;
    border-radius: 8px;
    box-shadow: 0 4px 5px #999;
    color: #fff;
    display: inline-block;
    font-size: 15px;
    font-weight: bold;
    padding: 10px 25px;
    text-decoration: none;
}
.single-pricing-box.light .pricing-btn a {
    background: #81d0ac none repeat scroll 0 0;
}
.single-pricing-box.light .pricing-btn a:hover {
    background: #57b188 none repeat scroll 0 0;
}

.single-pricing-box.business .pricing-btn a {
    background: #5AC0F1 none repeat scroll 0 0;
}
.single-pricing-box.business .pricing-btn a:hover {
    background: #2e96c8 none repeat scroll 0 0;
}

.single-pricing-box.expert .pricing-btn a {
    background: #1d50ff none repeat scroll 0 0;
}
.single-pricing-box.expert .pricing-btn a:hover {
    background: #0a32b9 none repeat scroll 0 0;
}





/* editing-transpro-area */
.editing-transpro-area {
    margin-bottom: 25px;
}
.transpro-content {
    background: #fcfcfc none repeat scroll 0 0;
    border: 1px solid #ddd;
    display: block;
    padding: 20px;
}
.transpro-content p {
	font-size: 13px;
	font-weight: bold;
	margin-bottom: 15px;
}
.transpro-box {
    margin-bottom: 15px;
}
.transpro-box h4 {
    font-size: 15px;
    font-weight: bold;
    margin: 0 0 3px;
}
.transpro-box p {
    border: medium none;
    margin: 0 0 5px;
    padding: 0;
}
.transpro-box img {
    width: 100%;
}



.transpro-box.light h4 {
    color: #81d0ac;
}
.transpro-box.expert h4 {
    color: #1d50ff;
}



.transpro-content form .form-control {
	border-radius: 0;
	display: inline-block;
	font-weight: normal;
	height: auto;
	padding: 2px;
	width: 160px;
}
.transpro-content i.fa-play {
    margin-left: 15px;
    padding-right: 10px;
}
.transpro-content i.fa-play-circle {
    padding-right: 10px;
}

.transpro-tab-area ul li a {
    font-size: 14px;
    font-weight: bold;
    text-transform: capitalize;
}
.tab-pane.transpro-tabpan {
    height: 300px;
}
.transpro-text-area textarea {
    border: 1px solid #ddd;
    overflow: auto;
    padding: 15px;
    resize: vertical;
    width: 100%;
}

.transpro-fileupload-area{
    height:230px;
}
.tab-pane.transpro-tabpan p {
    color: #f26522;
    font-weight: normal;
    margin: 0;
}

.transpro-tab-btn {
background: #f6f6f6 none repeat scroll 0 0;
border: 1px solid #ddd;
padding: 20px;
}

.transpro-tab-btn input[type="submit"] {
	background: #01bafe; /* Old browsers */
	background: -moz-linear-gradient(top, #01bafe 0%, #008bbf 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #01bafe 0%,#008bbf 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #01bafe 0%,#008bbf 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#01bafe', endColorstr='#008bbf',GradientType=0 ); /* IE6-9 */
	
	border:none;
    border-radius: 6px;
    box-shadow: 0 2px 3px #999;
    color: #fff;
    display: inline-block;
    font-size: 15px;
    font-weight: bold;
    padding: 12px 32px;
    text-decoration: none;
}

.transpro-tab-btn input[type="submit"]:hover {
	background: #008bbf; /* Old browsers */
	background: -moz-linear-gradient(top, #008bbf 1%, #01bafe 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #008bbf 1%,#01bafe 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #008bbf 1%,#01bafe 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#008bbf', endColorstr='#01bafe',GradientType=0 ); /* IE6-9 */
}



.feedback-wrap {
    padding: 10px;
}



.feedback-box {
	background: #fff none repeat scroll 0 0;
	border: 1px solid #ddd;
	margin-bottom: 15px;
	text-align: center;
	opacity: .75;
}
.feedback-box img {
    padding: 5px;
    width: 100%;
}
.stars {
    padding: 2px 0;
}
.stars i.fa {
	color: #ee7500;
	font-size: 18px;
	padding: 3px;
}
.feedback-name {
    background: #ddd none repeat scroll 0 0;
    padding: 5px;
}
.feedback-name p {
    color: #000;
    margin: 0;
}
.feedback-box:hover {
	opacity: 1;
}









.service-area .site-title {
    margin-bottom: 15px;
}
.single-service {
	background: #fffffe; /* Old browsers */
	background: -moz-linear-gradient(top, #fffffe 0%, #dddddd 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #fffffe 0%,#dddddd 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #fffffe 0%,#dddddd 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fffffe', endColorstr='#dddddd',GradientType=0 ); /* IE6-9 */
	border: 1px solid #ddd;
	border-radius: 10px;
	box-shadow: 0 3px 4px #666;
	display: block;
	margin-bottom: 30px;
	min-height: 136px;
	padding: 20px 20px 20px 115px;
	position: relative;
}
.single-service img {
    left: 15px;
    position: absolute;
    top: 30px;
}
.single-service h4 {
	color: #00a7e6;
	font-size: 20px;
	font-weight: bold;
	margin: 0 0 10px;
}
.single-service p {
    font-size: 14px;
    line-height: 20px;
    margin: 0;
}






.sidebar {
    margin-top: 15px;
}
.add-box {
    margin-bottom: 15px;
}
.add-box a img {
    transition: all 0.35s ease 0s;
    width: 100%;
}
.add-box a img:hover {
    box-shadow: 0 2px 3px #aaa;
}



.sidebar-faq {
    margin-bottom: 15px;
}
.sidebar-faq-heding a {
	background: #01baff; /* Old browsers */
	background: -moz-linear-gradient(top, #45b3dd 0%, #008bbf 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, #45b3dd 0%,#008bbf 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, #45b3dd 0%,#008bbf 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#01baff', endColorstr='#008bbf',GradientType=0 ); /* IE6-9 */
    color: #fff;
    display: block;
    font-size: 14px;
    font-weight: bold;
    padding: 7px 15px;
    text-decoration: none;
}
.sidebar-faq-content {
    background: #f6f6f6 none repeat scroll 0 0;
    border: 1px solid #ddd;
    padding: 10px;
}
.single-faq-box {
    margin-bottom: 15px;
}
.faq-q {
    margin-bottom: 3px;
    padding-left: 25px;
    position: relative;
}
.faq-q img {
    left: 0;
    position: absolute;
    top: 1px;
}
.faq-q p {
    font-weight: bold;
    margin: 0;
}
.faq-a {
    padding-left: 25px;
    position: relative;
}
.faq-a img {
    left: 0;
    position: absolute;
    top: 1px;
}
.faq-a p {
    line-height: 20px;
    margin: 0;
}



.sidebar-box.fb .sidebar-content {
    padding: 0;
}
.sidebar-box.fb .sidebar-content img {
    width: 100%;
}
.editor-interview .sidebar-content img {
    float: left;
    padding-right: 10px;
}
.editor-interview .sidebar-content p {
    line-height: 20px;
    margin: 0 0 5px;
}






.footer-top {
    background: #f0f0f0 none repeat scroll 0 0;
    padding: 40px 0;
}
.site-map h4 {
    font-size: 14px;
    font-weight: bold;
    margin: 0 0 10px;
}
.site-map h4 a {
    color: #333;
    text-decoration: none;
}
.site-map h4 a:hover {
    color: #0078d7;
}
.sitemap-menu {
}
.sitemap-menu ul {
    list-style: outside none none;
    margin: 0;
    padding: 0;
}
.sitemap-menu ul li {
    padding-bottom: 3px;
}
.sitemap-menu ul li a {
    color: #666;
    font-size: 11px;
    text-decoration: none;
}
.sitemap-menu ul li a i.fa {
    padding-right: 5px;
}
.sitemap-menu ul li a:hover {
    color: #0078d7;
}



.footer-copywrite {
    background: #1a74b3 none repeat scroll 0 0;
    color: #fff;
    padding: 20px 0;
}
.footer-copywrite h5 {
    font-size: 13px;
    margin: 0 0 5px;
}
.footer-copywrite p {
    margin: 0;
}
.fa.fa-close{
    border-radius: 50%;
    width: 25px;
    border: 0;
    background: white;
    height: 25px;
}
.fa.fa-close:hover{
    background: #e6e6e6;
}
.uib-tab.nav-item {
    font-size:14px;
    font-weight:bold;
}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a{
   background: #45b3dd none repeat scroll 0 0;
    color: #fff;
    padding: 5px;  
    font-size:12px; 
}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover{
   background: #45b3dd none repeat scroll 0 0;
    color: #fff;
    padding: 5px;
}



/* User Registration page CSS start */
.user-reg-wrap {
    background: #fff none repeat scroll 0 0;
    border: 1px solid #ddd;
    padding: 15px;
}
.user-reg-wrap h3 {
	color: #00A7E6;
    font-size: 16px;
	font-weight:bold;
	margin: 10px 0 15px 0;
}
.user-reg-content {
    background: #f6f6f6 none repeat scroll 0 0;
    border: 1px solid #ddd;
    margin-bottom: 30px;
    padding: 15px 15px 0;
}
.user-reg-content p {
    margin-bottom: 25px;
    text-align: justify;
}
.user-reg-content p button {
    display: inline-block;
    font-size: 12px;
    font-weight: bold;
    padding: 5px 12px;
    text-transform: capitalize;
}
.user-box {
    margin-bottom: 25px;
}
.single-user-box {
    border: 1px solid #ddd;
    border-radius: 10px;
    box-shadow: 0 0 3px 2px #eee;
    min-height: 240px;
    padding: 15px;
    position: relative;
	transition: .35s;
}
.single-user-box:hover {
    border-radius: 10px;
    box-shadow: 0 0 5px 3px #ccc;
}
.single-user-box h3 {
    border-bottom: 1px dashed #ddd;
    color: #444;
    font-size: 16px;
    font-weight: bold;
    line-height: 22px;
    margin: 0 0 15px;
    padding-bottom: 15px;
}
.single-user-box ul {
    list-style: outside none none;
    margin: 0;
    padding: 0;
}
.single-user-box ul li {
    padding-bottom: 5px;
}
.user-btn {
    bottom: 15px;
    left: 0;
    position: absolute;
    text-align: center;
    width: 100%;
}
.terms-box {
    background: #f6f6f6 none repeat scroll 0 0;
    border: 1px solid #ddd;
    padding: 15px;
}

.terms-box p button {
    font-size: 12px;
    font-weight: bold;
    padding: 5px 12px;
}
/* User Registration page CSS end */







