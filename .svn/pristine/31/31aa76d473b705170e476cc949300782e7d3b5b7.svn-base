<div class="total-content">
    <!-- Report-errors-page-area start -->
    <div class="individuals-reg-area">
        <div class="site-title">
            <i class="fa fa-th-large"></i>
            <h3>Report errors of webisite</h3>
        </div>
        <div class="individuals-reg-wrap">
            <form action="">
                <div class="reg-row">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i> E-mail address</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" E-mail Address" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i> Name</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" Name" />
                                    <span>Ex) Taro Yamada</span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i> Error pageのURL</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" Error URL" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i> エラーMessage</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" Error message" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i> OS</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" OS" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i>Browser and version</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder=" Web Browser" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x">
                                <p><i class="fa fa-arrow-circle-right"></i>何度も発生しますか？(再現性)</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <input type="text" placeholder="何度も発生しますか？(再現性)" />
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0">
                    <div class="row">
                        <div class="col-sm-3 pr5">
                            <div class="item-name h1x p3x">
                                <p><i class="fa fa-arrow-circle-right"></i> *エラーContents （できるだけ詳しく記入してください）</p>
                            </div>
                        </div>
                        <div class="col-sm-9 pl5 ">
                            <div class="item-value h1x">
                                <p class="red-text wd70">
                                    <textarea name="" rows="2" cols="20" id="errorDescription" placeholder=" Error Description" style="height:250px; width:98%;  max-width:100%; min-width: 200px;"></textarea>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-trams text-center">
                    <input type="submit" value="Send" />
                </div>
            </form>
        </div>
    </div>
</div>