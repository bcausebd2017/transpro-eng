                                       min-date="dateFrom"
                                       max-date="dateTo"
                                       close-text="Close"
                                       ng-model="vm.model.BirthDate"
                                       show-weeks="true"
                                       is-open="dateFromOpened"
                                       ng-click="dateFromOpened = true"
                                       filter-by="placed"
                                       filter-type="dateFrom" />
                                <span class="input-group-btn">
                                    <label for="birthDate" class="btn btn-default remove-padding">
                                        <i class="fa fa-calendar"></i>
                                    </label>
                                </span>
                            </div>
                        </div>                                             
                   </div>
