<div class="total-content">
    <!-- user-registration-translator-area start -->
    <div class="individuals-reg-area">
        <div class="site-title">
            <i class="fa fa-th-large"></i>
            <h3 ng-if="!vm.IsModifyState">Translator Registration</h3>
            <h3 ng-if="vm.IsModifyState">Rearrange Registered Information </h3>
        </div>
        <div class="individuals-reg-wrap">
            <form class="form-horizontal" name="translatorForm">
                <div class="spinner" ng-if="!isTranslatorLoaded">
                    <div class="bounce1"></div>
                    <div class="bounce2"></div>
                    <div class="bounce3"></div>
                </div>
                <h3>Sections with * need to be entered</h3>
                <h4>Account</h4>
                <div class="reg-row">
                    <div class="row">
                        <div class="col-sm-3 pr0">
                            <div class="item-name h5x p2x">
                                <p><i class="fa fa-arrow-circle-right"></i> <span>*</span>E-mail address</p>
                            </div>
                        </div>
                        <div class="col-sm-8 pl5">
                            <div class="item-value h5x">
                                <p>
                                    <input type="text" ng-model="vm.model.StaffEmailID" name="regemail" uib-tooltip="Please enter your email address"
                                             tooltip-placement="top"  ng-pattern="/^[a-z]+[a-z0-9._]+@[a-z]+\.[a-z.]{1,5}$/"
                                            tooltip-trigger="none"  ng-readonly="vm.IsModifyState" 
                                            ng-class="{'invalid:-input':isTriedSave && translatorForm.regemail.$error.required }" required/>

                                    <span ng-if="vm.IsOperationCompleted  && !vm.translatorExisted"><i style="font-size:18px;color:green;" class="fa fa-check-circle"></i></span>
                                    <span><input ng-if="!vm.IsModifyState" type="button" value="ID Check" class="btn btn-primary" ng-click="checkExistingTranslator()" /></span> </p>

                                <p style="color:red;" ng-show="translatorForm.email.$error.pattern">
                                    Invalid email!
                                </p>
                                <p ng-if="vm.IsOperationCompleted  && vm.translatorExisted" ng-style="{'color':'red'}">Email ID already used.</p>
                                <p class="red-text" ng-if="!vm.IsModifyState">
                                    <input type="text" ng-model="vm.RegEmail2" name="regemail2" ng-class="{'invalid:-input':isTriedSave && translatorForm.regemail2.$error.required }" required />
                                    <span>* Enter again</span></p>
                                <p ng-if="vm.RegEmail2.length > 0 && vm.model.StaffEmailID!== vm.RegEmail2" ng-style="{'color':'red'}">Email does not match</p>
                                
                            </div>
                        </div>
                    </div>
                </div>
                <div class="reg-row bt0" ng-if="!vm.IsModifyState">
                    <div class="row">
                        <div class="col-sm-3 pr0">
                            <div class="item-name h5x p2x">
                                <p><i class="fa fa-arrow-circle-right"></i> <span>*</span>PW</p>
                            </div>
                        </div>
                        <div class="col-sm-8 pl5">
                            <div class="item-value h5x">
                                <p class="red-text">
                                    <input class="input-pass" type="password" name="password" ng-minlength="8" ng-model="vm.model.Password" ng-class="{'invalid-input':isTriedSave && translatorForm.password.$error.required || $ctrl.vm.model.Password < 8}" required /><span ng-show="translatorForm.password.$error.minlength" class="help-block">Password is too short.Please enter more than 8 characters.</span>
                                 </p>
                               
                                <p class="red-text">
                                    <input type="password" id="repass" name="repass" ng-model="vm.Repass" ng-class="{'invalid-input':isTriedSave && translatorForm.repass.$error.required}" required /> <span>* Enter again </span> </p>
                                <p ng-if="vm.Repass.length > 0 && vm.model.Password !== vm.Repass" ng-style="{'color':'red'}">Password does not match</p>
                            </div>
                        </div>
                    </div>
                </div>
                <h4 class="mt30">Your information</h4>
                <div class="reg-row">
                    <div class="row">
                        <div class="col-sm-3 pr0">
                            <div class="item-name h1x">
