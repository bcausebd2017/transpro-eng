<div class="total-content">
    <div class="my-page-area">
        <div class="site-title">
            <i class="fa fa-th-large"></i>
            <h3>Reissue password</h3>
        </div>
        <div class="individuals-reg-wrap">
            <form name="loginCustomerForm">
                <!--<span style="color:red;" ng-show="isInvalidUser">{{'INVALIDUSER' | translator}}</span>-->
                <div class="row">
                    <div class="form-group has-feedback">
                        <div class="col-sm-12 col-md-12 col-xs-12">
                            <div class="col-sm-4 col-md-4 col-xs-4 text-right">
                                <label>Enter Email</label>
                            </div>
                            <div class="col-sm-7 col-md-7 col-xs-7">
                                <input type="text" name="username" class="form-control" placeholder="ID@e-mail" required>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        <div class="col-xs-12 text-center">
                            <button ng-click="send()" class="btn btn-primary ">Send Email</button>
                        </div>
                    </div>
                    <!-- /.col -->
                    <!-- /.col -->

                </div>
            </form>
        </div>
    </div>
</div>
