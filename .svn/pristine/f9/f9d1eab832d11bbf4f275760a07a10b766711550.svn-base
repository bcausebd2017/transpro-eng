                                <input class="estimate-amount" type="text" name="" ng-if="Variable.IsPriceListAvailable == true" readonly
                                       value="{{LanguagePriceCategoryList[0].CurrencySymbol}} {{Estimation.Price | number: 2}}" />
							</div>
						</div>
						<div class="clearfix"></div>
