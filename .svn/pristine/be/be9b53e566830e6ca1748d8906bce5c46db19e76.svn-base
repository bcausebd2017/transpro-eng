    background: -moz-linear-gradient(top, #d7fbca 0%, #c4efb2 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(top, #d7fbca 0%,#c4efb2 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom, #d7fbca 0%,#c4efb2 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d7fbca', endColorstr='#c4efb2',GradientType=0 ); /* IE6-9 */
    padding: 16px 0px 3px;
    box-shadow:3px 1px 3px #999;
    border: 2px solid #ace894;
    margin-bottom: 20px;
  }

  .appstyle2 {
    background: #a7dbfa; /* Old browsers */
    background: -moz-linear-gradient(top, #a7dbfa 0%, #83c8ef 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(top, #a7dbfa 0%,#83c8ef 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom, #a7dbfa 0%,#83c8ef 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#a7dbfa', endColorstr='#83c8ef',GradientType=0 ); /* IE6-9 */
    padding: 16px 0px 3px;
    box-shadow:3px 1px 3px #999;
    border: 2px solid #5cb8ed;
  }

  .appstyle3 {
    background: #e9cef1; /* Old browsers */
    background: -moz-linear-gradient(top, #e9cef1 0%, #f0defb 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(top, #e9cef1 0%,#f0defb 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom, #e9cef1 0%,#f0defb 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e9cef1', endColorstr='#f0defb',GradientType=0 ); /* IE6-9 */
    padding: 16px 0px 3px;
    box-shadow:3px 1px 3px #999;
    border: 2px solid#d7a0e8;
  }

  .appstyle1 h4, .appstyle2 h4, .appstyle3 h4 {
    color: #000;
    font-size: 23px;
    font-weight: 300;
    letter-spacing: -1px;
    text-align: center;
  }

  .appstylesml{
    border: 2px solid#A7C5D6;
    padding: 25px 0;
    margin: 0 auto;
    margin-bottom: 10px;
    margin-top:10px;
    max-width: 500px;
     /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#fdfefe+26,eff6f9+100 */
    background: #fdfefe; /* Old browsers */
    background: -moz-linear-gradient(top, #fdfefe 26%, #eff6f9 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(top, #fdfefe 26%,#eff6f9 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom, #fdfefe 26%,#eff6f9 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fdfefe', endColorstr='#eff6f9',GradientType=0 ); /* IE6-9 */
  }

  .appstylesml h4{
    color: #103f58;
    font-size: 19px;
    font-weight: 400;
    letter-spacing: 1px;
    margin: 0;
  }

  .appstylesml span {
    color: #308DC3;
    font-size: 30px;
  }

  .appstylesmlX {
    background: rgba(0, 0, 0, 0) linear-gradient(to bottom, #fdfefe 26%, #eff6f9 100%) repeat scroll 0 0;
    border: 2px solid #a7c5d6;
    margin: 10px auto;
    max-width: 400px;
  }

  .appstylesmlX h4{
    color: #103f58;
    font-size: 16px;
    font-weight: 400;
    letter-spacing: 1px;
    margin: 0;
  }

  .appstylesmlX span {
    color: #308DC3;
    font-size: 30px;
  }

  