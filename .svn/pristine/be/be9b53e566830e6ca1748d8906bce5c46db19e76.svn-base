/* header-top-area */
.header-top-area {
    /*padding: 5px 0;*/
    position: fixed;
    top: 0;
    width: 100%;
    background: #fff;
    z-index: 10000;
    border-top: 3px solid rgb(175, 209, 71);
    height:41px;
}
.header-top-area strong{
