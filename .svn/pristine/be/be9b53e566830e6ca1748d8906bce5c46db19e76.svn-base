    border-radius: 0px 0px 0px 50px;
    height: 22px;
    padding-left:10px;
}
.top-left p {
    font-size: 12px;
    margin: 0;
}
.top-right ul {
    display: inline-block;
    list-style: outside none none;
    /*margin: 0 0px 0 0;*/
    padding: 0;
}
.top-right ul li {
    border-right: 1px solid #333;
    display: inline-block;
    line-height: 12px;
    padding-left: 10px;
    padding-right: 10px;
}
.top-right ul li:last-child {
